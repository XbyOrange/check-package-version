{
  "name": "@nex/check-package-version",
  "version": "2.0.2",
  "dependencies": {
    "chalk": "2.4.1"
  },
  "bin": {
    "check-version-not-published": "./bin/check-version-not-published",
    "check-version-in-changelog": "./bin/check-version-in-changelog",
    "check-package-version": "./bin/check-package-version"
  },
  "scripts": {
