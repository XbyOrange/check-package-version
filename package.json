    "husky": "0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2",
    "sinon": "^6.3.2"
  },
  "engines": {
    "node": ">8.0.0",
    "npm": ">6.0.0"
  },
  "license": "MIT",
  "repository": "https://gitlab.com/OrangeX/frontend/tools/check-package-version"
